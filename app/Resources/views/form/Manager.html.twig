{% extends "::base.html.twig" %}
{% form_theme form ':form/block:field.html.twig' %}
{% block content %}

    <div class="col-md-5">
        {{ form_start(form)}}

        <h1 class="h1">Менеджер проекта</h1>

        {{ form_row(form.name) }}
        {{ form_row(form.email) }}
        {{ form_row(form.phone) }}

        <div class="next-btn">
            <button type="submit" class="btn btn-default">
                <span class="next-text">Далее ></span>
            </button>
        </div>


        {{ form_end(form) }}
    </div>

{% endblock %}